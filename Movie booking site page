function myFunction(number) {
 if(number==1)
 {
   document.getElementById("movie").innerHTML = "This movie if of Salman and Disha patani";
 }
 else if(number==2)
 {
       document.getElementById("movie").innerHTML = "This is the snyder verse of Justice League";
 }
   else if(number==3)
 {
       document.getElementById("movie").innerHTML = "This is the 3rd movie of Godzilla and kong";
 }
 
}
